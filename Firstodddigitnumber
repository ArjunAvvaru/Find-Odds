#include <iostream>
#include <cstdio>
using namespace std;
int firstOddDigitNumber(int a[], int n){
  int res,count=0;
  for(int i=0;i<n;i++){
    if(a[i]==0){
    return a[i];
    }
  	res=a[i];
    while(a[i]!=0)
    {
    count++;
      a[i]/=10;   
    }
    if(count%2!=0)
    {
    return res;
    }
  }
   return -1;
}
